<Window xmlns="https://github.com/avaloniaui"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:titlebars="clr-namespace:TestComponents.Views.TitleBar;assembly=TestComponents"
        mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
        x:Class="TestComponents.Views.Information"
        Title="Information"
        ExtendClientAreaToDecorationsHint="True"
ExtendClientAreaChromeHints="NoChrome"
ExtendClientAreaTitleBarHeightHint="-1"
        >
  <DockPanel HorizontalAlignment="Stretch"
            VerticalAlignment="Stretch"
            Name="ContentWrapper">
    <titlebars:WindowsTitleBar IsSeamless="False"></titlebars:WindowsTitleBar>
    <titlebars:MacosTitleBar IsSeamless="False"></titlebars:MacosTitleBar>
    <DockPanel Background="Transparent">
      <StackPanel HorizontalAlignment="Center"
                  VerticalAlignment="Center"
                  Spacing="10">
      </StackPanel>

    </DockPanel>
  </DockPanel>
</Window>
